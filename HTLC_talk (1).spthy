  BlockchainHeight( ('1'+BlockchainHeight) ),
  BlockchainTrace( BlockchainName, BlockchainID, preBlockchainState,
                   BChash(<preBlockchainState, TXID_3>), ('1'+BlockchainHeight)
  )
  ]->
