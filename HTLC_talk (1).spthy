restriction Equality:
  "∀ x y #i. (Eq( x, y ) @ #i) ⇒ (x = y)"
  // safety formula

restriction inequality:
  "∀ x #i. (InEq( x, x ) @ #i) ⇒ (⊥)"
  // safety formula

restriction CheckHTLC_Spend_Script:
  "∀ #i SigVerifyA SigVerifyB HashVerify HTLChash SigVerify.
    (CheckHTLC_Spend_Script( SigVerifyA, SigVerifyB, HashVerify,
                             HTLChash, SigVerify
     ) @ #i) ⇒
    (((SigVerifyA = true) ∧ (SigVerifyB = true)) ∨
     ((HTLChash = HashVerify) ∧ (SigVerify = true)))"
  // safety formula

restriction Blockchain_amount_control_2:
  "∀ #i #j #k BlockchainName1 BlockchainName2 BlockchainName3.
    (((!BlockchainID_register( BlockchainName1 ) @ #i) ∧
      (!BlockchainID_register( BlockchainName2 ) @ #j)) ∧
     (!BlockchainID_register( BlockchainName3 ) @ #k)) ⇒
    (((#i = #j) ∨ (#j = #k)) ∨ (#k = #i))"
  // safety formula

restriction BlockchainName_Unique:
  "∀ #i #j BlockchainName BlockchainID1 BlockchainID2.
    ((Initial_Blockchain( BlockchainName, BlockchainID1 ) @ #i) ∧
     (Initial_Blockchain( BlockchainName, BlockchainID2 ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction Blockchain_height:
  "∀ #i BlockchainHeight.
    (BlockchainHeight( ('1'+'1'+'1'+'1'+'1'+'1'+'1'+BlockchainHeight)
     ) @ #i) ⇒
    (⊥)"
  // safety formula

restriction only_one_main_blockchain:
  "∀ #i #j BlockchainName BlockchainID preBlockchainState
     NewBlockchainState1 NewBlockchainState2 BlockchainHeight1
     BlockchainHeight2.
    ((BlockchainTrace( BlockchainName, BlockchainID,
                       preBlockchainState, NewBlockchainState1, BlockchainHeight1
      ) @ #i) ∧
     (BlockchainTrace( BlockchainName, BlockchainID, preBlockchainState,
                       NewBlockchainState2, BlockchainHeight2
      ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction Only_one_blcok_at_each_height:
  "∀ #i #j TX_random0 BlockchainName BlockchainID BlockHeight
     TX_random1.
    ((((BlockchainHeight( BlockHeight ) @ #i) ∧
       (Add_Tx( TX_random0, BlockchainName, BlockchainID ) @ #i)) ∧
      (BlockchainHeight( BlockHeight ) @ #j)) ∧
     (Add_Tx( TX_random1, BlockchainName, BlockchainID ) @ #j)) ⇒
    (#i = #j)"
  // safety formula

restriction NoDoubleSpending:
  "∀ #i #k TxID BlockchainID BlockchainName.
    ((Add_Tx( TxID, BlockchainName, BlockchainID ) @ #i) ∧
