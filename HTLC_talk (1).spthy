theory HTLC_talk begin

// Function signature and definition of the equational theory E

builtins: multiset
functions: BChash/1, HTLChash/1, Txhash/1, fst/1, h/1, h1/1, h4/1,
           pair/2, pk/1, sign/2, snd/1, true/0, verify/3
equations:
    fst(<x.1, x.2>) = x.1,
    snd(<x.1, x.2>) = x.2,
    verify(sign(x.1, x.2), x.1, pk(x.2)) = true



rule (modulo E) address_register:
   [ Fr( ~ltkA ) ]
  -->
   [ !Ltk( $A, ~ltkA ), !Pk( $A, pk(~ltkA) ), Out( pk(~ltkA) ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) BlockchainName_register:
   [ Fr( ~BlockchainName ) ]
  --[ !BlockchainID_register( ~BlockchainName ) ]->
   [ !BlockchainID_register( ~BlockchainName ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) Initial_Tx:
   [
   Fr( ~Nonce ), !Pk( A, pkA ),
   !BlockchainID_register( BlockchainName )
   ]
  --[
  Inital_TxID( BlockchainName,
               Txhash(<~Nonce, '0', pkA, '0', BlockchainName>)
  ),
  !Transaction( ~Nonce, '0', pkA, '0', BlockchainName,
                Txhash(<~Nonce, '0', pkA, '0', BlockchainName>)
  )
  ]->
   [
   !Transaction( ~Nonce, '0', pkA, '0', BlockchainName,
                 Txhash(<~Nonce, '0', pkA, '0', BlockchainName>)
   ),
   Out( ~Nonce ), Out( pkA ), Out( BlockchainName ),
   Out( Txhash(<~Nonce, '0', pkA, '0', BlockchainName>) )
   ]

  /* has exactly the trivial AC variant */

rule (modulo E) transactions:
   [
   UTXOonBlock( PreTXID, sigPre, pkA, '0', BlockchainName, TXID ),
   !Ltk( A, ltkA ), !Pk( B, pkB )
   ]
  -->
   [
   !Transaction( TXID,
                 sign(<TXID, pkA, pkB, '0', BlockchainName>, ltkA), pkB, '0',
                 BlockchainName,
                 Txhash(<TXID, sign(<TXID, pkA, pkB, '0', BlockchainName>, ltkA), 
                         pkB, '0', BlockchainName>)
   ),
   Out( TXID ),
   Out( sign(<TXID, pkA, pkB, '0', BlockchainName>, ltkA) ),
   Out( pkB ), Out( '0' ), Out( BlockchainName ),
   Out( Txhash(<TXID, 
                sign(<TXID, pkA, pkB, '0', BlockchainName>, ltkA), pkB, '0', 
                BlockchainName>)
   )
   ]

  // loop breaker: [0]
  /* has exactly the trivial AC variant */

rule (modulo E) Blockchain_init:
   [
   Fr( ~BlockchainID ),
   !Transaction( PreTXID, '0', pkA, '0', BlockchainName, TXID ),
   !BlockchainID_register( BlockchainName )[+]
   ]
  --[
  Add_Tx( TXID, BlockchainName, ~BlockchainID ),
  Initial_Blockchain( BlockchainName, ~BlockchainID ),
  !TransactionOnBlock( PreTXID, '0', pkA, '0', BlockchainName, TXID,
                       BChash(<BlockchainName, ~BlockchainID, TXID>)
  ),
  BlockchainHeight( '1' ),
  BlockchainTrace( BlockchainName, ~BlockchainID, '0',
                   BChash(<BlockchainName, ~BlockchainID, TXID>), '1'
  )
  ]->
   [
   !Blockchain( BlockchainName, ~BlockchainID,
                BChash(<BlockchainName, ~BlockchainID, TXID>), '1'
   ),
   !TransactionOnBlock( PreTXID, '0', pkA, '0', BlockchainName, TXID,
                        BChash(<BlockchainName, ~BlockchainID, TXID>)
   ),
   UTXOonBlock( PreTXID, '0', pkA, '0', BlockchainName, TXID )
   ]

  /* has exactly the trivial AC variant */

rule (modulo E) NewBlockGeneration_add_Normal_transaction_on_normal_block:
   [
   !TransactionOnBlock( PreTXID, PreSig, pkA, timelock,
                        BlockchainName, TXID, BlockchainState
   ),
   !Transaction( TXID, SigA, pkB, '0', BlockchainName, NewTXID ),
   !Blockchain( BlockchainName, BlockchainID, preBlockchainState,
                BlockchainHeight
   )[+]
   ]
  --[
  Add_Tx( NewTXID, BlockchainName, BlockchainID ),
  Eq( verify(SigA, <TXID, pkA, pkB, '0', BlockchainName>, pkA), true
  ),
  !TransactionOnBlock( TXID, SigA, pkB, '0', BlockchainName, NewTXID,
                       BChash(<preBlockchainState, NewTXID>)
  ),
  BlockchainHeight( ('1'+BlockchainHeight) ),
  BlockchainTrace( BlockchainName, BlockchainID, preBlockchainState,
                   BChash(<preBlockchainState, NewTXID>), ('1'+BlockchainHeight)
  )
  ]->
   [
   !Blockchain( BlockchainName, BlockchainID,
                BChash(<preBlockchainState, NewTXID>), ('1'+BlockchainHeight)
   ),
   !TransactionOnBlock( TXID, SigA, pkB, '0', BlockchainName, NewTXID,
                        BChash(<preBlockchainState, NewTXID>)
   ),
   UTXOonBlock( TXID, SigA, pkB, '0', BlockchainName, NewTXID )
   ]

