rule (modulo E) NewBlockGeneration_add_Initial_transaction_on_normal_block:
   [
   !Transaction( PreTXID, '0', pkA, '0', BlockchainName, NewTXID ),
   !Blockchain( BlockchainName, BlockchainID, preBlockchainState,
                BlockchainHeight
   )[+]
   ]
  --[
  Add_Tx( NewTXID, BlockchainName, BlockchainID ),
  !TransactionOnBlock( PreTXID, '0', pkA, '0', BlockchainName,
                       NewTXID, BChash(<preBlockchainState, NewTXID>)
  ),
  BlockchainHeight( ('1'+BlockchainHeight) ),
  BlockchainTrace( BlockchainName, BlockchainID, preBlockchainState,
                   BChash(<preBlockchainState, NewTXID>), ('1'+BlockchainHeight)
  )
  ]->
   [
   !Blockchain( BlockchainName, BlockchainID,
                BChash(<preBlockchainState, NewTXID>), ('1'+BlockchainHeight)
   ),
   !TransactionOnBlock( PreTXID, '0', pkA, '0', BlockchainName,
                        NewTXID, BChash(<preBlockchainState, NewTXID>)
   ),
   UTXOonBlock( PreTXID, '0', pkA, '0', BlockchainName, NewTXID )
   ]

