                                 <TXID_1, <pkA, pkB, HTLC_hash, pkB_2>, PKaddress, timelock, 
                                  BlockchainName>,
                                 pkA),
                          verify(Script2,
                                 <TXID_1, <pkA, pkB, HTLC_hash, pkB_2>, PKaddress, timelock, 
                                  BlockchainName>,
                                 pkB),
                          HTLChash(Script1), HTLC_hash,
                          verify(Script2,
                                 <TXID_1, <pkA, pkB, HTLC_hash, pkB_2>, PKaddress, timelock, 
                                  BlockchainName>,
                                 pkB_2)
  ),
  Add_Tx( TXID_3, BlockchainName, BlockchainID ),
  SpendTX( TXID, SigA, <pkA, pkB, HTLC_hash, pkB_2>, '0',
           BlockchainName, TXID_1, BlockchainState
