  /* has exactly the trivial AC variant */

rule (modulo E) NewBlockGeneration_normal_tx_to_script_transaction:
   [
   !TransactionOnBlock( PreTXID, PreSig, pkA_0, timelock,
                        BlockchainName, TXID, BlockchainState
   ),
   !ScriptTransaction( TXID, SigA_0, <pkA, pkB, HTLC_hash, pkB_2>,
                       '0', BlockchainName, TXID_1
   )[+],
   !Blockchain( BlockchainName, BlockchainID, preBlockchainState,
                BlockchainHeight
   )[+]
   ]
  --[
  Eq( verify(SigA_0,
             <TXID, pkA_0, <pkA, pkB, HTLC_hash, pkB_2>, '0', BlockchainName>,
             pkA),
      true
  ),
  Add_Tx( TXID_1, BlockchainName, BlockchainID ),
  Add_1st_or_2nd_HTLC( HTLC_hash ),
  !ScriptTransactionOnBlockchain( TXID, SigA_0,
                                  <pkA, pkB, HTLC_hash, pkB_2>, '0', BlockchainName, TXID_1,
                                  BChash(<preBlockchainState, TXID_1>)
  ),
  BlockchainHeight( ('1'+BlockchainHeight) ),
  BlockchainTrace( BlockchainName, BlockchainID, preBlockchainState,
                   BChash(<preBlockchainState, TXID_1>), ('1'+BlockchainHeight)
  )
  ]->
   [
   !Blockchain( BlockchainName, BlockchainID,
                BChash(<preBlockchainState, TXID_1>), ('1'+BlockchainHeight)
   )[+],
   !ScriptTransactionOnBlockchain( TXID, SigA_0,
                                   <pkA, pkB, HTLC_hash, pkB_2>, '0', BlockchainName, TXID_1,
                                   BChash(<preBlockchainState, TXID_1>)
   ),
   ScriptUTXOonBlock( TXID, SigA_0, <pkA, pkB, HTLC_hash, pkB_2>, '0',
                      BlockchainName, TXID_1
   )
   ]

